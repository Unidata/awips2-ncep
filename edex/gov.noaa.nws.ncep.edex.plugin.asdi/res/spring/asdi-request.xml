<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
    
    <bean id="RetrieveAsdiHandler"
        class="gov.noaa.nws.ncep.edex.plugin.asdi.decoder.RetrieveAsdiHandler" />
    <bean factory-bean="handlerRegistry" factory-method="register">
        <constructor-arg
            value="gov.noaa.nws.ncep.common.dataplugin.asdi.RetrieveAsdiRequest" />
        <constructor-arg ref="RetrieveAsdiHandler" />
    </bean>
    
</beans>
  