<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<purgeRuleSet>
	<!-- <key>typeInfo.source</key> -->
    <key>typeInfo.instrument</key>
    <key>typeInfo.datatype</key>
	<key>dataResolUnits</key>
	<key>dataResolVal</key>
	<!-- Default rule for data not covered by any rules, 2 versions up to 27 days-->
	<defaultRule>
		<versionsToKeep>2</versionsToKeep>
		<period>27-00:00:00</period>
	</defaultRule>
	<!-- specific product rules -->
 
	<!-- Purge rule for xrs xray 1-minute data -->
	<rule>
        <keyValue>xrs</keyValue>
        <keyValue>xray</keyValue>
		<keyValue>minutes</keyValue>
		<keyValue>1</keyValue>
		<period>81-00:00:00</period>
	</rule>
 
	<!-- Purge rule for mag magnetometer 1-minute data -->
	<rule>
        <keyValue>mag</keyValue>
		<keyValue>magnetometer</keyValue>
		<keyValue>minutes</keyValue>
		<keyValue>1</keyValue>
		<period>81-00:00:00</period>
	</rule>
 
    <!-- Purge rule for epead corrected-electrons 5-minutes data -->
    <rule>
        <keyValue>epead</keyValue>
        <keyValue>corrected-electrons</keyValue>
        <keyValue>minutes</keyValue>
        <keyValue>5</keyValue>
        <period>81-00:00:00</period>
    </rule>    
    
    <!-- Purge rule for epead differential-protons 5-minutes data -->
    <rule>
        <keyValue>epead</keyValue>
        <keyValue>differential-protons</keyValue>
        <keyValue>minutes</keyValue>
        <keyValue>5</keyValue>
        <period>81-00:00:00</period>
    </rule>
    
	<!-- Purge rule for epead integral-protons 5-minutes data -->
	<rule>
        <keyValue>epead</keyValue>
        <keyValue>integral-protons</keyValue>
		<keyValue>minutes</keyValue>
		<keyValue>5</keyValue>
		<period>81-00:00:00</period>
	</rule>
  
    <!-- Purge rule for sis particles 5-minutes data -->
    <rule>
        <keyValue>sis</keyValue>
        <keyValue>particles</keyValue>
        <keyValue>minutes</keyValue>
        <keyValue>5</keyValue>
        <period>81-00:00:00</period>
    </rule>
    
    <!-- Purge rule for epam electrons-protons 5-minutes data -->
    <rule>
        <keyValue>epam</keyValue>
        <keyValue>electrons-protons</keyValue>
        <keyValue>minutes</keyValue>
        <keyValue>5</keyValue>
        <period>81-00:00:00</period>
    </rule>    
    
    <!-- Purge rule for swepam solar-wind 1-minute data -->
    <rule>
        <keyValue>swepam</keyValue>
        <keyValue>solar-wind</keyValue>
        <keyValue>minutes</keyValue>
        <keyValue>1</keyValue>
        <period>81-00:00:00</period>
    </rule>
</purgeRuleSet>