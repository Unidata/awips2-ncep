            <midnightMeridianLineStyle>${midnightMeridianLineStyle}</midnightMeridianLineStyle>
            <dayShadeColor>${dayShadeColor}</dayShadeColor>
            <nightShadeColor>${nightShadeColor}</nightShadeColor>
            <shadeAlpha>${shadeAlpha}</shadeAlpha>
            <shadePattern>${shadePattern}</shadePattern>
