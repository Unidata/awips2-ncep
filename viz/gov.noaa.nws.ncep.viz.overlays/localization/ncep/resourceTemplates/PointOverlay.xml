<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<resourceGroup xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <resource>
          <properties isSystemResource="false" isBlinking="false" isMapLayer="true" isHoverOn="false" isVisible="true">
            <!--  would like to use the progressive Disclosure thresholds but we
                  have different values for the symbols and labels -->
            <pdProps maxDisplayWidth="100000000" minDisplayWidth="0" />
          </properties>
          <resourceData xsi:type="PointOverlayResourceData">
            <sourceType>${sourceType}</sourceType>
            <sourceName>${sourceName}</sourceName>
            <sourceParams>${sourceParams}</sourceParams>            
            <mapName>${mapName}</mapName>
            <legendColor>${color}</legendColor>
            <color>${color}</color>
            <markerState>${markerState}</markerState>
            <markerType>${markerType}</markerType>
            <markerSize>${markerSize}</markerSize>
            <markerWidth>${markerWidth}</markerWidth>
            <markerTextSize>${markerTextSize}</markerTextSize>    
            <maxSymbolDisplayWidth>${maxSymbolDisplayWidth}</maxSymbolDisplayWidth>
            <maxLabelDisplayWidth>${maxLabelDisplayWidth}</maxLabelDisplayWidth>        
          </resourceData>
        </resource>
</resourceGroup>
