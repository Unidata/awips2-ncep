<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<resourceGroup xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <resource>
          <loadProperties loadWithoutData="true"></loadProperties>
          <properties isSystemResource="false"
              isBlinking="false" isMapLayer="false" isHoverOn="false"
              isVisible="true" />
          <resourceData xsi:type="NC-WarnResourceData"
                        isUpdatingOnMetadataOnly="false" 
                        isRequeryNecessaryOnTimeMatch="false"> 
             <frameSpan>${frameSpan}</frameSpan>
             <dfltNumFrames>${dfltNumFrames}</dfltNumFrames>
             <dfltTimeRange>${dfltTimeRange}</dfltTimeRange>
             <timeMatchMethod>${timeMatchMethod}</timeMatchMethod>       
             <timelineGenMethod>${timelineGenMethod}</timelineGenMethod> 
             <sourceName>WARN</sourceName>
             <color>${color}</color>     
             <thunderstormEnable>${thunderstormEnable}</thunderstormEnable>
             <thunderstormColor>${thunderstormColor}</thunderstormColor>
             <thunderstormSymbolWidth>${thunderstormSymbolWidth}</thunderstormSymbolWidth>
			 <thunderstormSymbolSize>${thunderstormSymbolSize}</thunderstormSymbolSize>
             <tornadoEnable>${tornadoEnable}</tornadoEnable>
             <tornadoColor>${tornadoColor}</tornadoColor>
             <tornadoSymbolWidth>${tornadoSymbolWidth}</tornadoSymbolWidth>
             <tornadoSymbolSize>${tornadoSymbolSize}</tornadoSymbolSize>
             <flashFloodEnable>${flashFloodEnable}</flashFloodEnable>
             <flashFloodColor>${flashFloodColor}</flashFloodColor>
             <flashFloodSymbolWidth>${flashFloodSymbolWidth}</flashFloodSymbolWidth>
             <flashFloodSymbolSize>${flashFloodSymbolSize}</flashFloodSymbolSize>
             <timeEnable>${timeEnable}</timeEnable>
             <countyNameEnable>${countyNameEnable}</countyNameEnable>
             <countyOutlineEnable>${countyOutlineEnable}</countyOutlineEnable>
             <stormBasedPolygonEnable>${stormBasedPolygonEnable}</stormBasedPolygonEnable>
             <requestStyle>METADATA</requestStyle>
             <metadataMap>
               <mapping key="pluginName">
                 <constraint constraintValue="${pluginName}" constraintType="EQUALS" />
               </mapping>
               <mapping key="reportType">
                 <constraint constraintValue="${reportType}" constraintType="IN" />
               </mapping>
             </metadataMap>
          </resourceData>
        </resource>
</resourceGroup>

