<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<resourceGroup xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <resource>
    <loadProperties loadWithoutData="true"></loadProperties>
    <properties isSystemResource="false" isBlinking="false"
      isMapLayer="false" isHoverOn="false" isVisible="true" />
    <resourceData xsi:type="NC-PgenResourceData"
      isUpdatingOnMetadataOnly="false" isRequeryNecessaryOnTimeMatch="false">
      <frameSpan>${frameSpan}</frameSpan>
      <dfltNumFrames>${dfltNumFrames}</dfltNumFrames>
      <dfltTimeRange>${dfltTimeRange}</dfltTimeRange>
      <timeMatchMethod>${timeMatchMethod}</timeMatchMethod>       
      <timelineGenMethod>${timelineGenMethod}</timelineGenMethod> 
      <legendColor>${color}</legendColor>
      <monoColorEnable>${monoColorEnable}</monoColorEnable>
      <monoColor>${monoColor}</monoColor>
      <fillModeEnable>${fillModeEnable}</fillModeEnable>
      <metadataMap>
        <mapping key="pluginName">
          <constraint constraintValue="pgen" constraintType="EQUALS" />
        </mapping>
        <mapping key="dataTime">
          <constraint constraintValue="${dataTime}" constraintType="EQUALS" />
        </mapping>
        <mapping key="activityName">
          <constraint constraintValue="${activityName}" constraintType="EQUALS" />
        </mapping>
        <mapping key="activityType">
          <constraint constraintValue="${activityType}" constraintType="EQUALS" />
        </mapping>
        <mapping key="activitySubtype">
          <constraint constraintValue="${activitySubtype}" constraintType="EQUALS" />
        </mapping>
        <mapping key="activityLabel">
          <constraint constraintValue="${activityLabel}" constraintType="EQUALS" />
        </mapping>
        <mapping key="site">
          <constraint constraintValue="${site}" constraintType="EQUALS" />
        </mapping>
        <mapping key="desk">
          <constraint constraintValue="${desk}" constraintType="EQUALS" />
        </mapping>
        <mapping key="forecaster">
          <constraint constraintValue="${forecaster}" constraintType="EQUALS" />
        </mapping>
        <mapping key="operatingMode">
          <constraint constraintValue="${operatingMode}" constraintType="EQUALS" />
        </mapping>
      </metadataMap>
    </resourceData>
  </resource>
</resourceGroup>        
