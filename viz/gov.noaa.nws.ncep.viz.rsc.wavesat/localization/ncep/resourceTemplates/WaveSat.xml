<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<resourceGroup xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <resource>
    <loadProperties>
    </loadProperties>
    <properties isSystemResource="false" isBlinking="false"
      isMapLayer="false" isHoverOn="false" isVisible="true" />
    <resourceData xsi:type="NC-WaveSatResourceData"
      retrieveData="false" isUpdatingOnMetadataOnly="false"
      isRequeryNecessaryOnTimeMatch="false">

      <legendColor>${legendColor}</legendColor>
      <frameSpan>${frameSpan}</frameSpan>
      <timeMatchMethod>${timeMatchMethod}</timeMatchMethod>
      <timelineGenMethod>${timelineGenMethod}</timelineGenMethod> 
      <dfltNumFrames>${dfltNumFrames}</dfltNumFrames>
      <dfltTimeRange>${dfltTimeRange}</dfltTimeRange>
      <fontName>${fontName}</fontName>
      <fontSize>${fontSize}</fontSize>
      <timeDisplayInterval>${timeDisplayInterval}</timeDisplayInterval>
      <timeDisplayColor>${timeDisplayColor}</timeDisplayColor>
      <useFeetInsteadOfMeters>${useFeetInsteadOfMeters}</useFeetInsteadOfMeters>
      <latParam>CLATH</latParam>   
      <latParamInPdo>clath</latParamInPdo> 
      <lonParam>CLONH</lonParam>
      <lonParamInPdo>clonh</lonParamInPdo>
      <satIdParam>SAID</satIdParam>
      <satIdParamInPdo>said</satIdParamInPdo>
      <waveHeightParam>SGWH</waveHeightParam>
      <windSpeedParam>WS10G3R1</windSpeedParam>
      
      ${colorBarForMeters}
      
      ${colorBarForFeet}
      
      <metadataMap>
        <mapping key="pluginName">
          <constraint constraintValue="${pluginName}" constraintType="EQUALS" />
        </mapping>
        <mapping key="said">
          <constraint constraintValue="${satelliteId}" constraintType="EQUALS" />
        </mapping>
      </metadataMap>
    </resourceData>
  </resource>
</resourceGroup>
