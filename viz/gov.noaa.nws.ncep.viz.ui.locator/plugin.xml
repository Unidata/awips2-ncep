   <extension
          point="com.raytheon.uf.viz.localization.localizationpath">
       	<path
             application="NCEP"
             localizationType="CAVE_STATIC"
             name="LocatorDataSources"
             recursive="false"
             value="ncep/LocatorDataSources"
	         extensionFilter=".xml">
       </path>
      </extension>
   
</plugin>
