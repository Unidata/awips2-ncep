            ncResourceName="Locator">
      </nc-resourceParameter>
      <nc-resourceParameter
            paramClass="String"
            paramName="pos2DirectionUnit"
            paramType="EDITABLE_ATTRIBUTE"
            ncResourceName="Locator">
      </nc-resourceParameter>
      
      <nc-resourceParameter
            paramClass="String"
